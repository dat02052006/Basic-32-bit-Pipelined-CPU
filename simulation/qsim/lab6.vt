module CPU_pipeline_vlg_vec_tst();
// constants                                           
// general purpose registers
reg CLK;
reg Reset;
// wires                                               
wire [31:0] ALUresult;
wire [31:0] Instruction_o;
wire [31:0] PC_out;
wire [31:0] WriteData;

wire sampler;                             

// assign statements (if any)                          
CPU_pipeline i1 (
// port map - connection between master ports and signals/registers   
	.ALUresult(ALUresult),
	.CLK(CLK),
	.Instruction_o(Instruction_o),
	.PC_out(PC_out),
	.Reset(Reset),
	.WriteData(WriteData)
);

// Reset
initial
begin
	Reset = 1'b0;
end 

// CLK
always
begin
	CLK = 1'b0;
	CLK = #20000 1'b1;
	#20000;
end 

CPU_pipeline_vlg_sample_tst tb_sample (
	.CLK(CLK),
	.Reset(Reset),
	.sampler_tx(sampler)
);

CPU_pipeline_vlg_check_tst tb_out(
	.ALUresult(ALUresult),
	.Instruction_o(Instruction_o),
	.PC_out(PC_out),
	.WriteData(WriteData),
	.sampler_rx(sampler)
);
endmodule

