module CPU_pipeline_vlg_sample_tst(
	CLK,
	Reset,
	sampler_tx
);
input  CLK;
input  Reset;
output sampler_tx;

reg sample;
time current_time;
always @(CLK or Reset)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

